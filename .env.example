# Server Configuration
NODE_ENV=development
PORT=3000

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:3001

# Backend
DATABASE_URL=postgresql://user:password@localhost:5432/connectivebyte
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=1d

# Lab Module
LAB_DATABASE_URL=postgresql://user:password@localhost:5432/connectivebyte_lab
LAB_ENCRYPTION_KEY=replace-with-32-byte-hex-key
LAB_SHARED_OPENAI_KEY=optional-shared-key
LAB_REDIS_URL=redis://localhost:6379

# Optional: External Services (uncomment and configure as needed)
# AWS_ACCESS_KEY_ID=your_aws_access_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key
# AWS_REGION=ap-northeast-1
# S3_BUCKET_NAME=your-bucket-name

# Optional: Analytics
# GOOGLE_ANALYTICS_ID=UA-XXXXX-Y
# SENTRY_DSN=your_sentry_dsn
